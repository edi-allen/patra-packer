{
	"agent": {
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/nginx/access.log",
						"log_group_name": "access.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/var/log/nginx/error.log",
						"log_group_name": "error.log",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
}
